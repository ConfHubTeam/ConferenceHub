{
  "name": "airbnb-clone",
  "version": "1.0.0",
  "description": "Airbnb Clone PERN Stack",
  "main": "api/index.js",
  "scripts": {
    "start": "cd api && node index.js",
    "dev:api": "cd api && nodemon index.js",
    "dev:client": "cd client && npm run dev",
    "dev": "concurrently \"npm run dev:api\" \"npm run dev:client\"",
    "build": "cd client && npm install && npm run build",
    "render-postbuild": "npm install"
  },
  "dependencies": {
    "@cloudinary/react": "^1.14.1",
    "@cloudinary/url-gen": "^1.21.0",
    "axios": "^1.5.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "cloudinary": "^2.6.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "image-downloader": "^4.3.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.5.0",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.14.1",
    "react-router-dom": "^6.15.0",
    "sequelize": "^6.37.7",
    "sequelize-cli": "^6.6.2",
    "streamifier": "^0.1.1",
    "xtypejs": "^0.7.1",
    "@vitejs/plugin-react": "^4.0.3"
  },
  "devDependencies": {
    "concurrently": "^8.2.1",
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=14"
  }
}
